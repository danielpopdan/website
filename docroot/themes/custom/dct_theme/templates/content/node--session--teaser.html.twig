<div class="schedule-item">
    <div class="schedule-item--detail-row">
        <div class="schedule-item--room">{{ content.field_room }}</div>
        <div class="schedule-item--hours">{{ content.session_hours }}</div>
    </div>
    <div class="schedule-item--title">{{ content.display_title }}</div>
    <div class="schedule-item--speaker">{{ content.speakers }}</div>
    <div class="schedule-item--difficulty">
       {{ content.difficulty }}
    </div>
    <div class="schedule-item--detail-row">
        <div class="schedule-item--topic">{{ content.field_taxonomy_reference }}
        </div>
        {{ content.flag_add_to_my_schedule }}
    </div>
</div>
{{ content|without('field_room', 'field_taxonomy_reference', 'session_hours', 'display_title', 'speakers', 'difficulty', 'flag_add_to_my_schedule') }}
