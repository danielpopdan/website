{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6.5",
        "drupal-composer/drupal-scaffold": "^2.5",
        "drupal-generics/entity_theme_suggestions": "8.1.0-alpha1",
        "drupal-generics/invisible_recaptcha": "8.1.0-alpha2",
        "drupal/acquia_connector": "1.13",
        "drupal/address": "1.4",
        "drupal/addtoany": "1.10",
        "drupal/advagg": "3.5",
        "drupal/better_exposed_filters": "3.0-alpha4",
        "drupal/calendar": "1.x-dev",
        "drupal/coder": "8.3.1",
        "drupal/commerce": "2.10",
        "drupal/config_installer": "1.8",
        "drupal/console": "1.8",
        "drupal/contact_storage": "1.0-beta9",
        "drupal/core": "8.6.1",
        "drupal/entityqueue": "1.x-dev",
        "drupal/flag": "4.0-alpha3",
        "drupal/gdpr": "2.0-alpha5",
        "drupal/google_map_field": "1.5",
        "drupal/mailchimp": "1.7",
        "drupal/metatag": "1.7",
        "drupal/migrate_source_csv": "2.2",
        "drupal/migrate_tools": "4.x-dev",
        "drupal/paragraphs": "1.3",
        "drupal/pathauto": "1.3",
        "drupal/simple_pass_reset": "1.0",
        "drupal/simple_sitemap": "2.12.0",
        "drupal/swiftmailer": "1.0-beta1",
        "drush/drush": "^9.0.0",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3"
    },
    "require-dev": {
        "webflo/drupal-core-require-dev": "^8.6.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/{$name}": ["type:drupal-drush"]
        },
        "drupal-scaffold": {
            "initial": {
                ".editorconfig": "../.editorconfig",
                ".gitattributes": "../.gitattributes"
            }
        }
    }
}
