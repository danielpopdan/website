{#
/**
 * @file
 * Default theme implementation for a 'form' element.
 *
 * Available variables
 * - attributes: A list of HTML attributes for the wrapper element.
 * - children: The child elements of the form.
 *
 * @see template_preprocess_form()
 *
 * @ingroup themeable
 */
#}
<div>
    <img src="/{{ directory }}/images/backgrounds/small-header.png" alt="background">
</div>
<div class="article-section form-article">
    <div class="l-container">
        <div class="title-container">
            <div class="large-title-block title">
                <h2>{{ "Log in"|t }}</h2>
            </div>
        </div>
    </div>
</div>
<div class="sponsors-contact">
    <div class="l-container">
        <div class="small-title-block">
            <form{{ attributes }}>
                {{ children }}
            </form>
            <div class="forgot-password form-two-column">
                <a href="{{ path('user.register') }}" class="form-wrapper">{{ "Registration"|t }}</a>
                <a href="{{ path('user.pass') }}" class="form-wrapper">{{ "Forgot password?"|t }}</a>
            </div>
        </div>
    </div>
</div>
